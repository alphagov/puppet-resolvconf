<%
unless @nameservers.empty? or (@dhcp_enabled == 'true' && ! @override_dhcp)
  @nameservers.each do |ns|
-%>
nameserver <%= ns %>
<%
  end
end
-%>
<% unless @domain.empty? -%>
domain <%= @domain %>
<% end -%>
<% unless @search.empty? -%>
search <%= @search.join(' ') %>
<% end -%>
<% unless @options.empty? -%>
options <%= @options.join(' ') %>
<% end -%>
